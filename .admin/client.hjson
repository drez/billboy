			"date('Date')": ["date()"],
			"note('Note')": ["longvarchar(500)"],
			"amount('Amount')": ["decimal(8, 2)", 'default:0.00', 'required'],
			
			"validator": {
				"amount":{
					"minLength":{"msg":"payment_line_amount_required","value":"1"},
					}
				},
		},
