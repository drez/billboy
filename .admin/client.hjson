      "set_parent_table": 'billing',
      "set_readonly_columns":["total"],
			set_menu_priority: 1,

			"id_cost_line()": ["primary()"],
			"id_billing()": ["integer()","foreign(billing)","onDelete:cascade","foreign:id_billing",'not-required', 'default:null'],
